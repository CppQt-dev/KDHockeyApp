INCLUDEPATH += $$INCLUDEDIR/KDHockeyApp
DEPENDPATH += $$INCLUDEDIR/KDHockeyApp

KDHOCKEYAPP_LIBRARY_FILENAMES = \
    $$LIBDIR/$${QMAKE_PREFIX_STATICLIB}KDHockeyApp.$${QMAKE_EXTENSION_STATICLIB} \
    $$LIBDIR/$${QMAKE_PREFIX_STATICLIB}KDHockeyAppBreakpad.$${QMAKE_EXTENSION_STATICLIB}

!isEqual(TEMPLATE, lib)|CONFIG(shared, static|shared) {
    LIBS -= $$KDHOCKEYAPP_LIBRARY_FILENAMES
    LIBS += $$KDHOCKEYAPP_LIBRARY_FILENAMES
    PRE_TARGETDEPS *= $$KDHOCKEYAPP_LIBRARY_FILENAMES
}
